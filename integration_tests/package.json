    "compare:build": "node scripts/build-comparison-tools.js",
    "test:spec": "npm run integration --",
    "test": "npm run integration --",
    "chrome": "cd chrome_runner && npm test -- ",
    "snapshot-viewer": "node tools/snapshot-viewer/cli.js",
    "spec-preview": "node tools/spec-preview/cli.js",
    "tools:install": "cd tools/snapshot-viewer && npm install && cd ../spec-preview && npm install"
