{
  "name": "@openwebf/vue-router",
  "version": "0.24.1",
  "description": "Vue Router implementation for WebF applications using hybrid history API",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "test": "npm run build && node --test tests/pathMatcher.test.js tests/webfRouter.test.js tests/useRoutes.test.js tests/hybridRouterChange.test.js tests/routerLinkPrerendering.test.js",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openwebf/webf",
    "directory": "packages/vue-router"
  },
  "keywords": [
    "vue",
    "router",
    "webf",
    "navigation",
    "hybrid"
  ],
  "author": "WebF Team",
  "license": "Apache-2.0",
  "peerDependencies": {
    "vue": "^3.0.0",
    "@openwebf/webf-enterprise-typings": ">=0.22.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/node": "^18.0.0",
    "@vue/compiler-sfc": "^3.3.0",
    "rollup": "^3.20.0",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.0",
    "vue": "^3.3.0"
  },
  "type": "module"
}
