{
  "name": "@openwebf/webf-react-core-ui",
  "version": "0.1.0",
  "description": "Core UI components for WebF React applications",
  "main": "lib/index.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:types",
    "build:lib": "rollup -c",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir lib",
    "dev": "rollup -c -w",
    "clean": "rm -rf lib dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.9.6",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/openwebf/webf.git",
    "directory": "packages/webf-react-core-ui"
  },
  "keywords": [
    "webf",
    "react",
    "ui",
    "components",
    "flutter"
  ],
  "author": "OpenWebF Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/openwebf/webf/issues"
  },
  "homepage": "https://github.com/openwebf/webf#readme"
}