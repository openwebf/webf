{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["ES2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": false,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "types": [],
    "typeRoots": ["../"]
  },
  "include": [
    "*.test.ts",
    "*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}